{
  "name": "web_ntex",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/web/ntex/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@monodon/rust:build",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/web_ntex"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "test": {
      "executor": "@monodon/rust:test",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/web_ntex"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "executor": "@monodon/rust:lint",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/web_ntex"
      }
    },
    "run": {
      "executor": "@monodon/rust:run",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/web_ntex"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "docker-gcps": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "gcloud builds submit . --substitutions _IMAGE=web_ntex,_APP_NAME=web_ntex"
      }
    }
  }
}
