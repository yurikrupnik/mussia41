{
  "namedInputs": {
    "tfSource": [
      "{projectRoot}/src/*.tf"
    ],
    "tfWD": [
      "{projectRoot}/src/.terraform"
    ],
    "tfPlan": [
      "{projectRoot}/src/tfplan"
    ]
  },
  "targetDefaults": {
    "initialize": {
      "inputs": [
        "tfSource"
      ]
    },
    "lint": {
      "inputs": [
        "tfSource"
      ]
    },
    "validate": {
      "inputs": [
        "tfSource",
        "tfWD"
      ]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "initialize"
        ]
      }
    }
  }
}
